<!doctype html>
<html class="no-js" lang="en" dir="ltr">

<!-- Mirrored from pixelwibes.com/template/ebazar/html/dist/order-list.html by HTTrack Website Copier/3.x [XR&CO'2014], Thu, 08 Feb 2024 06:56:28 GMT -->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Admin - Product List </title>
    <link rel="icon" href="" type="">

    <!-- plugin css file  -->
    <link rel="stylesheet" href="/admin/assets/plugin/datatables/responsive.dataTables.min.css">
    <link rel="stylesheet" href="/admin/assets/plugin/datatables/dataTables.bootstrap5.min.css">

    <!-- project css file  -->
    <link rel="stylesheet" href="/admin/assets/css/ebazar.style.min.css">
</head>
<body>
    <div id="ebazar-layout" class="theme-blue">                          
        
        <!-- sidebar -->
        <div class="sidebar px-4 py-4 py-md-4 me-0">
          <div class="d-flex flex-column h-100">
            <a href="index.html" class="mb-0 brand-icon">
              <span class="logo-icon">
                <i class="bi bi-bag-check-fill fs-4"></i>
              </span>
              <span class="logo-text">WatchVista</span>
            </a>
            <!-- Menu: main ul -->
            <ul class="menu-list flex-grow-1 mt-3">
              <li>
                <a class="m-link" href="/admin/dashboard"
                  ><i class="icofont-home fs-5"></i> <span>Dashboard</span></a
                >
              </li>
                 <li class="collapsed">
              <a class="m-link" href="/admin/servicesList">
                <i class="icofont-truck-loaded fs-5"></i> <span>Services</span>
              </a>
            </li>
              <li>
                <a class="m-link" href="/admin/categoryList"
                  ><i class="icofont-chart-flow fs-5"></i>
                  <span>Category</span></a
                >
              </li>
              <li>
                <a class="m-link" href="/admin/subcategoryList"
                  ><i class="icofont-chart-flow fs-5"></i>
                  <span>Subcategory</span></a
                >
              </li>
  
                         <li>
              <a class="m-link" href="/admin/bookings">
                <i class="icofont-notepad fs-5"></i> <span>Bookings</span>
              </a>
            </li>
              <li class="collapsed"></li>
              <li>
                <a class="m-link" href="/admin/customersList"
                  ><i class="icofont-funky-man fs-5"></i> <span>Users</span></a
                >
              </li>
              <li>
                <a class="m-link" href="/admin/vendorsList">
                  <i class="icofont-funky-man fs-5"></i> <span>Vendors</span>
                </a>
              </li>
              <li>
                <a class="m-link" href="/admin/banner"
                  ><i class="icofont-page fs-5"></i> <span>Banner</span></a
                >
              </li>
            </ul>
          </div>
        </div>    

        <!-- main body area -->
        <div class="main px-lg-4 px-md-4"> 

            <!-- Body: Header -->
            <div class="header">
                <nav class="navbar py-4">
                    <div class="container-xxl">
        
                        <!-- header rightbar icon -->
                        <div class="h-right d-flex align-items-center mr-5 mr-lg-0 order-1">
                           
                            <div class="dropdown user-profile ml-2 ml-sm-3 d-flex align-items-center zindex-popover">
                                <div class="u-info me-2">
                                    <p class="mb-0 text-end line-height-sm "><span class="font-weight-bold"><%= user.name %></span></p>
                                    <small>WatchVista Admin</small>

                                </div>
                                <a class="nav-link dropdown-toggle pulse p-0" href="#" role="button" data-bs-toggle="dropdown" data-bs-display="static">
                                    <img class="avatar lg rounded-circle img-thumbnail" src="/admin/assets/images/profile_av.svg" alt="profile">
                                </a>
                                <div class="dropdown-menu rounded-lg shadow border-0 dropdown-animation dropdown-menu-end p-0 m-0">
                                    <div class="card border-0 w280">
                                        <div class="card-body pb-0">
                                            <div class="d-flex py-1">
                                                <img class="avatar rounded-circle" src="/admin/assets/images/profile_av.svg" alt="profile">
                                                <div class="flex-fill ms-3">
                                                    <p class="mb-0"><span class="font-weight-bold"><%= user.name %></span></p>
                                                    <small class=""><%= user.email %></small>
                                                </div>
                                            </div>
                                            
                                            <div><hr class="dropdown-divider border-dark"></div>
                                        </div>
                                        <div class="list-group m-2 ">
                          
                                            <a
                                            href="/admin/logout"
                                            class="list-group-item list-group-item-action border-0"
                                            ><i class="icofont-logout fs-5 me-3"></i>Signout</a
                                            >
                                                                
                                        </div>
                                    </div>
                                </div>
                            </div>
                           
                        </div>
        
                    
                        <!-- main menu Search-->
                        <div class="order-0 col-lg-4 col-md-4 col-sm-12 col-12 mb-3 mb-md-0 ">
                            <div class="input-group flex-nowrap input-group-lg">
                                <input type="search" class="form-control" placeholder="Search" aria-label="search" aria-describedby="addon-wrapping">
                                <button type="button" class="input-group-text" id="addon-wrapping"><i class="fa fa-search"></i></button>
                                
                            </div>
                        </div>
        
                    </div>
                </nav>
            </div>

            <!-- Body: Body -->
            <div class="body d-flex py-3">  
                <div class="container-xxl"> 
                    <div class="row align-items-center"> 
                        <div class="border-0 mb-4"> 
                            <div class="card-header py-3 no-bg bg-transparent d-flex align-items-center px-0 justify-content-between border-bottom flex-wrap">
                                <h3 class="fw-bold mb-0">Services List
                            </div>
                        </div>
                    </div> <!-- Row end  -->
                    <div class="row g-3 mb-3"> 
                        <div class="col-md-12">
                            <div class="card"> 
                                <div class="card-body"> 
                                    <table id="myDataTable" class="table table-hover align-middle mb-0" style="width: 100%;">  
                                        <thead>
                                            <tr>
                                                <th></th>
                                                <th>Image</th>
                                                <th>Title</th>
                                                <th>Price</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% services.forEach((service,index) => { %>
                                            <tr>
                                                <td><strong><%= index + 1 %></strong></td>
                                                <td><img src="<%= service.images[0] %>" class="avatar lg rounded " alt="<%= service.title %>"></td>
                                                <td><%= service.title %></td>
                                                <td>â‚¹ <%= service.sellingPrice %></td>
                                                <td>
                                                    <button class="btn bg-dark text-light" data-bs-toggle="modal" data-bs-target="#productModal<%= index %>">View</button>
                                                    <!-- Modal -->
                                                    <div class="modal fade" id="productModal<%= index %>" tabindex="-1" aria-labelledby="productModalLabel<%= index %>" aria-hidden="true">
                                                        <div class="modal-dialog modal-dialog-centered modal-lg">
                                                        <div class="modal-content">
                                                            <div class="modal-header">
                                                            <h5 class="modal-title" id="productModalLabel<%= index %>">Service Details</h5>
                                                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                                            </div>
                                                            <div class="modal-body">
                                                            <!-- Product details here -->
                                                            <div class="row">
                                                                <div class="col-lg-6">
                                                                    <img class="img-fluid" src="<%= service.images[0] %>" alt="<%= service.title %>">
                                                                </div>
                                                                <div class="col-lg-6">
                                                                    <div>
                                                                        <p><strong>Name :</strong> <%= service.title %></p>
                                                                        <p><strong>Price :</strong> <%= service.sellingPrice %></p>
                                                                        <p><strong>Category :</strong> <%= service.category %></p>
                                                                        <p><strong>Subcategory :</strong> <%= service.subcategory %></p>
                                                                        <p class="text-sm text-wrap text-break"><strong>Description :</strong> <%= service.description %></p>
                                                                    </div>
                                                                </div>
                                                            </div>                                                         
                                                            <!-- Add more product details as needed -->
                                                            </div>
                                                            <div class="modal-footer">
                                                            <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Close</button>
                                                            </div>
                                                        </div>
                                                        </div>
                                                    </div>
                                                </td>
                                            </tr>

                                            <% }) %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div> <!-- Row end  -->
                </div>
            </div>
        
            
            
            
        </div>                                 

    </div> 

    <!-- Jquery Core Js -->
    <script src="/admin/assets/bundles/libscripts.bundle.js"></script>

    <!-- Plugin Js -->
    <script src="/admin/assets/bundles/dataTables.bundle.js"></script>  

    <!-- Jquery Page Js -->
    <script src="../js/template.js"></script>
    <script>
        $('#myDataTable')
        .addClass( 'nowrap')
        .dataTable( {
            responsive: true,
            columnDefs: [
                { targets: [-1, -3], className: 'dt-body-right' }
            ]
        });
    </script>
</body>

