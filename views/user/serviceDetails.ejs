      <div id="loader"></div>
    </div>

    <!-- header section start -->
    <header
      class="header pt-30 pb-30 header-sticky header-static bg-white"
      style="padding-top: 30px; padding-bottom: 15px; top: 0px"
    >
      <div class="container-fluid">
        <div class="header-nav position-relative">
          <div class="row align-items-center">
            <div class="col-xl-5 col-lg-6 hidden-md position-static">
              <div class="header-nav">
                <nav>
                  <ul>
                    <li>
                      <a href="/">
                        <span>Home </span>
                      </a>
                    </li>

                    <li class="position-static">
                      <a href="/services">
                        <span>Services</span>
                      </a>
                    </li>

                    <li>
                      <a href="/contact">
                        <span>Contact</span>
                      </a>
                    </li>
                  </ul>
                </nav>
              </div>
            </div>
            <div class="col-xl-3 col-lg-3 col-md-6 col-sm-6 col-3">
              <div class="logo">
                <a href="index3.html"
                  ><img src="/img/logo/logo1.png" alt=""
                /></a>
              </div>
            </div>

            <div class="col-xl-4 col-lg-3 col-6 col-md-6 col-sm-6 col-9">
              <div class="header-right">
                <ul class="text-right">
                  <% if(!user){ %>
                  <li>
                    <a href="/login" class="account"
                      ><i class="fal fa-user-friends"></i>
                      <article class="account-registar d-inline-block">
                        Login
                      </article></a
                    >
                  </li>
                  <% } %>

                  <li>
                    <a><i class="fal fa-align-right"></i></a>
                    <ul class="submenu bold-content text-right">
                      <li><a href="/userProfile">My Account</a></li>

                      <% if(user){ %>
                      <li><a href="/user/logout">Logout</a></li>
                      <% } %>
                    </ul>
                  </li>
                </ul>
              </div>
            </div>
          </div>
        </div>
        <div class="mobile-menu visible-sm">
          <div id="mobile-menu">
            <ul>
              <li><a class="pl-3" href="/">Home</a></li>
              <li><a class="pl-3" href="/services">Services</a></li>
              <li><a href="/contact">Contact</a></li>
            </ul>
          </div>
        </div>
        <!-- /. mobile nav -->
      </div>
    </header>
    <!-- header section end -->

    <!-- single product start -->
    <section class="single-product mb-90">
      <div class="container-fluid">
        <div class="shop-wrapper">
          <div class="single-product-top">
            <div class="row">
              <div class="col-xl-6 col-lg-6 col-12">
                <div class="shop-img">
                  <div class="row">
                    <div class="col-8">
                      <div class="tab-content" id="v-pills-tabContent">
                        <div class="tab-pane fade show active" id="tab-1">
                          <div
                            class="product-img zoomable d-flex justify-content-center"
                          >
                            <a href="" class="popup-image"
                              ><img
                                src="<%= service.images[0] %>"
                                class="zoomable__img"
                                alt=""
                                style="width: 500px; height: auto"
                            /></a>
                          </div>
                        </div>
                        <div class="tab-pane fade" id="tab-2">
                          <div
                            class="product-img zoomable d-flex justify-content-center"
                          >
                            <a href="" class="popup-image"
                              ><img
                                src="<%= service.images[1] %>"
                                class="zoomable__img w-100"
                                alt=""
                                style="width: 500px; height: auto"
                            /></a>
                          </div>
                        </div>
                        <div class="tab-pane fade" id="tab-3">
                          <div
                            class="product-img zoomable d-flex justify-content-center"
                          >
                            <a href="" class="popup-image"
                              ><img
                                src="<%= service.images[2] %>"
                                class="zoomable__img w-100"
                                alt=""
                                style="width: 500px; height: auto"
                            /></a>
                          </div>
                        </div>
                        <div class="tab-pane fade" id="tab-4">
                          <div
                            class="product-img zoomable d-flex justify-content-center"
                          >
                            <a href="" class="popup-image"
                              ><img
                                src="<%= service.images[3] %>"
                                class="zoomable__img w-100"
                                alt=""
                                style="width: 500px; height: auto"
                            /></a>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="col-2">
                      <div
                        class="nav nav-pills has-border-img"
                        id="v-pills-tab"
                        role="tablist"
                        aria-orientation="vertical"
                      >
                        <a class="active img-" data-toggle="pill" href="#tab-1">
                          <img
                            src="<%= service.images[0] %>"
                            class="w-100"
                            alt=""
                          />
                        </a>
                        <a data-toggle="pill" href="#tab-2">
                          <img
                            src="<%= service.images[1] %>"
                            class="w-100 mt-5-px"
                            alt=""
                          />
                        </a>
                        <a data-toggle="pill" href="#tab-3">
                          <img
                            src="<%= service.images[2] %>"
                            class="w-100 mt-5-px"
                            alt=""
                          />
                        </a>
                        <a data-toggle="pill" href="#tab-4">
                          <img
                            src="<%= service.images[3] %>"
                            class="w-100 mt-5-px"
                            alt=""
                          />
                        </a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-xl-6 col-lg-6 col-12">
                <div class="single-product-sidebar">
                  <div class="product-content">
                    <div class="single-product-title">
                      <h2><%= service.title %></h2>
                    </div>
                    <div class="single-product-price">
                      ₹ <span><%= service.sellingPrice %>/-</span>
                    </div>

                    <a
                      href="/service/<%= service._id %>/book"
                      class="bg-primary px-4 mt-4 py-2 rounded d-flex justify-content-center align-items-center text-white"
                      >Book Now</a
                    >
                    <div class="single-product-action mt-35"></div>
                  </div>
                  <div class="single-product-category">
                    <ul>
                      <li class="mb-0"><a href="" class="title">Tags : </a></li>
                      <li class="mb-0">
                        <a href=""><%= service.category %></a>
                      </li>
                      <li class="mb-0">
                        <a href=""><%= service.subCategory %></a>
                      </li>
                    </ul>
                  </div>
                  <div class="mt-4">
                    <h6>Description</h6>
                    <p><%= service.description %></p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- single product end -->

    <!-- related product start -->
    <% if(relatedServices?.length > 0) { %>
    <section class="related-product mt-120 mb-100">
      <div class="container-fluid">
        <div class="generic-title text-center">
          <h2 class="mb-20">Related Services</h2>
        </div>

        <div class="main-product-carousel owl-carousel red-nav mt-50">
          <% relatedServices.forEach((service) => { %>

          <div class="col-xl-3 col-lg-3 col-md-4 col-sm-6 col-12">
            <div class="product-box mb-40">
              <div class="product-box-wrapper">
                <div class="product-img">
                  <a
                    href="/service/<%= service._id %>"
                    class="product-category"
                  >
                    <img
                      src="<%= service.images[0] %>"
                      class="w-100"
                      alt="<%= service.title %>"
                    />
                  </a>
                </div>

                <div class="product-desc pb-20">
                  <a href="/service/<%= service._id %>" class="product-title"
                    ><%= service.title %></a
                  >
                  <span
                    >₹
                    <%= service.sellingPrice %></span
                  >
                </div>
              </div>
              <a
                href="/service/<%= service._id %>/book"
                class="bg-primary px-4 py-2 rounded d-flex justify-content-center align-items-center text-white"
                >Book Now</a
              >
            </div>
          </div>
          <% }) %>
        </div>
      </div>
    </section>
    <% } %>

    <!-- related product end -->

